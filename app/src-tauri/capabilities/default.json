{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "notification:default",
    "fs:read-all",
    "fs:write-all",
    {
      "identifier": "fs:scope",
      "allow": [
        "$HOME/**",
        "$DESKTOP/**",
        "$DOCUMENT/**",
        "$DOWNLOAD/**"
      ]
    }
  ]
}